body {
    background-color: #f2f2f2;
}

.head {
  height: 300px;
  background-image: url("Header.png");
  background-repeat: no-repeat;
  background-position: right top;
}

h3 {
  font-family: Arial;
  font-size: 20px;
  text-align: left;
  color: #ffffff;
}

ul.respnav {
    list-style-type: none;
    margin: 0;
    padding: 0;
    overflow: hidden;
    background-color: #333333;
}

ul.respnav li {float: left;}

ul.respnav li a {
    display: block;
    color: #ffffff;
    font-family: Arial;
    text-transform: uppercase;
    text-align: center;
    padding: 14px 16px;
    text-decoration: none;
}

ul.respnav li a:hover:not(.active) {background-color: #111111;}

ul.respnav li a.active {background-color: #1A96D4;}

ul.respnav li.right {float: left;}

@media screen and (max-width: 600px){
    ul.respnav li.right,
    ul.respnav li {float: none;}
}

.block1 {
  margin-top: 30px;
}

.datecheck {
  margin: auto;
  border-radius: 10px;
  width: 50%;
  height: 75px;
  width: 500px;
  padding: 50px 25px 30px 100px;
  background-color: #c6c6c6;
  box-shadow: 5px 5px 3px #777777;
}

.from {
  color: #5C5C5C;
  display: inline-block;
}

.too {
  color: #5C5C5C;
  display: inline-block;
}

input[type=text] {
    width: 100%;
    padding: 12px 20px;
    margin: 8px 0;
    box-sizing: border-box;
    border: 3px solid #CCCCCC;
    background-image: url('Calendar.png');
    background-repeat: no-repeat;
    -webkit-transition: 0.5s;
    transition: 0.5s;
    outline: none;
}

input[type=text]:focus {
    border: 3px solid #555555;
}
